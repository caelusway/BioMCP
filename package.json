{
  "name": "@eliza/bio-analytics-mcp",
  "version": "1.0.0",
  "description": "Model Context Protocol server for Bio/Science DAO analytics - Twitter, Discord, Telegram, Governance, and Liquidity metrics for Biotechnology DAOs",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:railway": "node scripts/railway-start.js",
    "dev": "tsc && node dist/index.js",
    "dev:watch": "tsc --watch",
    "railway:logs": "railway logs --follow",
    "railway:debug": "node scripts/railway-debug.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "bio",
    "biotechnology",
    "dao",
    "analytics",
    "science",
    "desci",
    "decentralized-science",
    "twitter",
    "discord",
    "telegram",
    "governance",
    "liquidity"
  ],
  "author": "Bio MCP Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.2",
    "@supabase/supabase-js": "^2.39.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  }
}
